{
  "private": false,
  "name": "react-swapy",
  "version": "1.0.0",
  "type": "module",
  "description": "A react wrapper for the swapy library by taha-sh - https://swapy.tahazsh.com/",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swapy": "^0.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "terser": "^5.31.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vishnusankaran/react-swapy.git"
  },
  "keywords": [
    "swap",
    "reorder",
    "drag",
    "draggable",
    "droppable",
    "dragndrop",
    "smooth",
    "animation",
    "swapy",
    "react-swapy"
  ],
  "license": "MIT"
}